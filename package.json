{
  "name": "deoptigate",
  "version": "0.5.0",
  "description": "Investigates v8/Node.js function deoptimizations.",
  "bin": "./bin/deoptigate",
  "main": "deoptigate.js",
  "files": [
    "app/build",
    "app/lib",
    "bin/",
    "lib/",
    "deoptigate*"
  ],
  "scripts": {
    "lint": "prettier -c {bin,examples,lib}/**/*.js app/{components,lib}/**/*.js app/main.js deoptigate.js deoptigate.log.js",
    "lint:fix": "prettier --write {bin,examples,lib}/**/*.js app/{components,lib}/**/*.js app/main.js deoptigate.js deoptigate.log.js",
    "test": "tape test/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thlorenz/deoptigate.git"
  },
  "homepage": "https://github.com/thlorenz/deoptigate",
  "dependencies": {
    "ansicolors": "~0.3.2",
    "files-provider": "~0.2.0",
    "ispawn": "~0.2.0",
    "mkdirp": "~0.5.1",
    "opn": "~5.4.0",
    "v8-tools-core": "~0.1.1"
  },
  "keywords": [],
  "author": {
    "name": "Thorsten Lorenz",
    "email": "thlorenz@gmx.de",
    "url": "http://thlorenz.com"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/thlorenz/deoptigate/blob/master/LICENSE"
  },
  "engine": {
    "node": ">=8"
  },
  "devDependencies": {
    "escape-string-regexp": "^4.0.0",
    "prettier": "^2.0.5",
    "tape": "^5.0.0"
  }
}
